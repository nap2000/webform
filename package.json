        "grunt-contrib-watch": "^1.1.0",
        "grunt-eslint": "^23.0.0",
        "grunt-karma": "^4.0.2",
        "grunt-sass": "^3.1.0",
        "grunt-shell": "3.0.1",
        "i18next-parser": "^3.11.0",
